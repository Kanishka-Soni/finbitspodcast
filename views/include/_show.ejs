<% for(var i = 0; i < users.length; i++) { %>
<tr>
    <td><%= i + 1 %></td>
    <td><%= users[i].name %></td>
    <td><%= users[i].content %></td>
    <td><%= users[i].imagelink %></td>
    <td><%= users[i].url %></td>
    <td><%= users[i].type %></td>
    <td><%= users[i].length %></td>
    <td><%= users[i].date %></td>
    <td>
        <a href="/update-news?id=<%= users[i]._id%>" class="btn border-shadow update">
            <span class="text-gradient">Edit</span>
        </a>
        <a class="btn border-shadow delete" data-id=<%= users[i]._id%> >
            <span class="text-gradient">Delete</span>
        </a>
    </td>
</tr>
<% } %>